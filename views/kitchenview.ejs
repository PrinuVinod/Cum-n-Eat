<!-- views/kitchenview.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Order List | Tasty</title>
</head>
<body>
  <h1>Order List</h1>
  <ul>
    <% orderList.forEach(order => { %>
      <li><%= order.itemName %> - Quantity: <%= order.quantity %></li>
    <% }); %>
  </ul>
</body>
</html>
